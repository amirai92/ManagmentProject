@model WebApplication3.ViewModel.VM
@using WebApplication3.Models

<h2>Looking for a job Board</h2>
*Add an AD<br />
*filter by disability

<table class="table" id="LookingTable">

    <tr>
        <td>Employee First Name</td>
        <td>Employee Last Name</td>
        <td>Phone Number</td>
        <td>Email</td>
        <td>Cv</td>
    </tr>



    
        @foreach (LookingAd ad in Model.LookingAds)
        {
            <tr>
                <td>@ad.FirstName</td>
                <td>@ad.LastName</td>
                <td>@ad.Phone</td>
                <td>@ad.Email</td>
                <td>@ad.Cv</td>
                <td></td>
            </tr>

        }


    

</table>
